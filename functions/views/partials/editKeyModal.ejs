<div class="modal fade" id="editKeyModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Edit Virtual Key</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form class="form editKeyForm" data-device-id="">
                <div class="form-group">
                    <label for="editKeyName">Name: </label>
                    <input type="text" id="editKeyName" name="name" class="form-control editKeyNameInput">
                </div>
                <div class="form-group">
                    <label for="editKeyExpiration">Expiration: </label>
                    <input type="text" class="form-control datePicker" id="editKeyExpiration" name="newExpiry">
                </div>
                <input type="hidden" name="userId" value="<%= userId %>">
                <input type="hidden" name="keyId" value="">
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            <button type="button" class="btn btn-primary editKeySubmit">Save Changes</button>
        </div>
        </div>
    </div>
</div>