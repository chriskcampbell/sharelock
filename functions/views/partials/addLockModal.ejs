<div class="modal fade" id="addLockModal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Lock</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form addDeviceForm">
                    <div class="form-group">
                        <label for="addLockName"><span class="requiredFieldAsterisk">*</span>Name: </label>
                        <input type="text" id="addLockName" name="name" class="form-control addLockNameInput">
                    </div>
                    <div class="form-group">
                        <label for="addLockAddress"><span class="requiredFieldAsterisk">*</span>Location (Address): </label>
                        <input type="text" id="addLockAddress" name="location" class="form-control addLockAddressInput">
                    </div>
                    <div class="form-group">
                        <label for="addLockSerial"><span class="requiredFieldAsterisk">*</span>Product Serial Number: </label>
                        <input type="text" id="addLockSerial" name="deviceId" class="form-control addLockSerialInput">
                    </div>
                    <input type="hidden" name="userId" value="<%= userId %>">
                </form>
            </div>
            <div class="modal-footer">
                <span class="requiredFieldDescriptor"><span class="requiredFieldAsterisk">*</span>Required Field</span>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary addLockSubmit">Submit</button>
            </div>
            </div>
        </div>
    </div>